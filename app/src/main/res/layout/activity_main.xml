<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.e.bogachov.unlimitedhouse.MainActivity"
    android:background="@drawable/background">



    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="287dp"
        android:id="@+id/imageView"
        android:background="@drawable/house"
        android:contentDescription="@string/deck"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="70dp"
        android:layout_marginLeft="98dp"
        android:layout_marginRight="94dp"/>

    <Button
        android:background="@drawable/backbuttoneng"
        android:layout_width="215dp"
        android:layout_height="66dp"
        android:id="@+id/BtnLogin"
        android:layout_below="@+id/imageView"
        android:layout_marginLeft="57dp"
        android:layout_marginTop="75dp"
       />

    <Button
        android:background="@drawable/butnarab"
        android:layout_width="215dp"
        android:layout_height="66dp"
        android:id="@+id/button"
        android:layout_below="@+id/imageView"
        android:layout_alignTop="@+id/BtnLogin"
        android:layout_alignLeft="@id/BtnLogin"
        android:layout_marginLeft="326dp"
        />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView2"
        android:layout_below="@+id/BtnLogin"
        android:layout_marginTop="21dp"
        android:layout_marginRight="37dp"
        android:layout_marginLeft="52dp"
        android:layout_alignLeft="@id/BtnLogin"
        android:contentDescription="@string/deck2"
        android:src="@drawable/mline" />

    <ImageButton
        android:background="@drawable/siginup"
        android:layout_width="225dp"
        android:layout_height="225dp"
        android:id="@+id/imageButton"
        android:layout_alignRight="@+id/BtnLogin"
        android:layout_alignTop="@id/imageView2"
        android:layout_marginTop="140dp"/>



</RelativeLayout>
